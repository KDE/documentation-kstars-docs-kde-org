#: ../../user_manual/fits-viewer-solver.rst:92
msgid "FITS Viewer Solver"
msgstr "Résolveur de l'afficheur FITS"

#: ../../user_manual/fits-viewer-solver.rst:5
msgid "|FITS Viewer Solver|"
msgstr ""

#: ../../user_manual/fits-viewer-solver.rst:92
