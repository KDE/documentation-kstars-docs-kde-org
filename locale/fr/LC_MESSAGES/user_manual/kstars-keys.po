msgid "**1**: Mercury"
msgstr "1  : Mercure"

#: ../../user_manual/kstars-keys.rst:28
msgid "**2**: Venus"
msgstr "2  : Venus"

#: ../../user_manual/kstars-keys.rst:30
msgid "**3**: Moon"
msgstr "3  : Lune"

#: ../../user_manual/kstars-keys.rst:32
msgid "**4**: Mars"
msgstr "4  : Mars"

#: ../../user_manual/kstars-keys.rst:34
msgid "**5**: Jupiter"
msgstr "5  : Jupiter"

#: ../../user_manual/kstars-keys.rst:36
msgid "**6**: Saturn"
msgstr "6  : Saturne"

#: ../../user_manual/kstars-keys.rst:38
msgid "**7**: Uranus"
msgstr "7  : Uranus"

#: ../../user_manual/kstars-keys.rst:40
msgid "**8**: Neptune"
msgstr "8  : Neptune"

#: ../../user_manual/kstars-keys.rst:42
msgid "**9**: Pluto"
msgstr "9  : Pluton"

