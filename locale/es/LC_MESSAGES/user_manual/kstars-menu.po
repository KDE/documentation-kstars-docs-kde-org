#. Tag: menuchoice
#: commands.docbook:338
#, no-c-format
msgid ""
"<shortcut><keycap>F6</keycap></shortcut><guimenuitem>Azimuthal Equidistant</"
"guimenuitem>"
msgstr ""
"<shortcut><keycap>F6</keycap></shortcut><guimenuitem>Acimutal equidistante</"
"guimenuitem>"

#. Tag: para
#: commands.docbook:339 commands.docbook:1395
#, no-c-format
