msgstr "<title>Razširitve</title>"

#. Tag: primary
#: ekos-extensions.docbook:4
#, no-c-format
msgid "Tools"
msgstr "Orodja"

#. Tag: secondary
#: ekos-extensions.docbook:5
#, no-c-format
msgid "Ekos"
msgstr "Ekos"

#. Tag: tertiary
#: ekos-extensions.docbook:6
#, no-c-format
msgid "<tertiary>Extensions</tertiary>"
