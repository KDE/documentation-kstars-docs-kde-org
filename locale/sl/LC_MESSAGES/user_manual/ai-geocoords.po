#. Tag: title
#: geocoords.docbook:8
#, no-c-format
msgid "Geographic Coordinates"
msgstr "Geografske koordinate"

#. Tag: primary
#: geocoords.docbook:9
#, no-c-format
msgid "<primary>Geographic Coordinate System</primary>"
msgstr "<primary>Geografski koordinatni sistem</primary>"

#. Tag: primary
#: geocoords.docbook:10
#, no-c-format
msgid "Longitude"
msgstr "Dolžina"

#. Tag: see
#: geocoords.docbook:10 geocoords.docbook:11
