msgid "Tools"
msgstr "Verktyg"

#. Tag: secondary
#: ekos-capture.docbook:5
#, no-c-format
msgid "Ekos"
msgstr "Ekos"

#. Tag: tertiary
#: ekos-capture.docbook:6
#, no-c-format
