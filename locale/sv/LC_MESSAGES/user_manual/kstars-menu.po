#: commands.docbook:119
#, no-c-format
msgid "<guimenu>Time</guimenu> <guimenuitem>Stop Clock</guimenuitem>"
msgstr "<guimenu>Tid</guimenu> <guimenuitem>Stoppa klockan</guimenuitem>"

